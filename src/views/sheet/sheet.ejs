<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <link rel="stylesheet" href="/stylesheets/index.css" />
        <title><%= name %></title>
    </head>
    <body>
        <input type="text" name="sheetNameInput" size="40" value="<%= name %>"/>
        <input type="button" name="saveButton" value="Enregistrer"/>
        <table>
            <thead>
                <tr>
                    <th></th>
                    <% for(var i = 1; i < 11; i++) {%>
                        <th><%= i %></th>
                    <% }%>
                </tr>
            </thead>
            <tbody>
                <% for(var i = 1; i < 11; i++) {%>
                    <tr>
                        <th><%= i %></th>
                        <% for(var j = 1; j < 11; j++) {%>
                            <td>
                                <input type="text" id="cell" name="<%= i %>_<%= j %>" size="10" />
                            </td>
                        <% }%>
                    </tr>
                <% }%>
            </tbody>
        </table>
    </body>
</html>